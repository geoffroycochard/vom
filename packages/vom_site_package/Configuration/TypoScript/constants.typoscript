############
### PAGE ###
############
page {
    logo {
        file = EXT:vom_site_package/Resources/Public/Images/logo.svg
        fileInverted = EXT:vom_site_package/Resources/Public/Images/logo-inverted.svg
        height = 52
        width = 192
        alt = VoM Site Package
        linktitle = VoM Site Package
    }
    favicon {
        file = EXT:vom_site_package/Resources/Public/Icons/favicon.ico
    }
    fluidtemplate {
        layoutRootPath = EXT:vom_site_package/Resources/Private/Layouts/Page/
        partialRootPath = EXT:vom_site_package/Resources/Private/Partials/Page/
        templateRootPath = EXT:vom_site_package/Resources/Private/Templates/Page/
    }
}


##################
### EXTENSIONS ###
##################
plugin.bootstrap_package {
    view {
        layoutRootPath = EXT:vom_site_package/Resources/Private/Layouts/
        partialRootPath = EXT:vom_site_package/Resources/Private/Partials/
        templateRootPath = EXT:vom_site_package/Resources/Private/Templates/
    }
}


